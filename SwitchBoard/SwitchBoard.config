set(startmode ${PLUGIN_SWITCHBOARD_STARTMODE})

if(PLUGIN_SWITCHBOARD_DEFAULT OR PLUGIN_SWITCH_AMAZON OR PLUGIN_SWITCH_COBALT OR PLUGIN_SWITCH_NETFLIX OR PLUGIN_SWITCH_WEBKITBROWSER)
  map()
  end()
  ans(configuration)
