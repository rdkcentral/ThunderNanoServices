set (autostart true)
set (preconditions Platform Network)
set (PLUGIN_SECURESHELLSERVER_IMPLEMENTATION "LibDropbear")

map()
    if (${PLUGIN_SECURESHELLSERVER_IMPLEMENTATION} STREQUAL "LibDropbear")
        kv(inputparameters "-R -p 22")
    endif ()
    if (PLUGIN_SECURESHELLSERVER_MODE)
        kv(mode ${PLUGIN_SECURESHELLSERVER_MODE})
    else()
        if (PLUGIN_SECURESHELLSERVER_OUTOFPROCESS)
            kv(mode "Local")
        else()
            kv(mode "Off")
        endif()
    endif()
end()
ans(configuration)
