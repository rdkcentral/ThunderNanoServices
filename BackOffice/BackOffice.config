    if(PLUGIN_BACKOFFICE_SERVER_PORT)
        kv(server_port ${PLUGIN_BACKOFFICE_SERVER_PORT})
    endif()
    if(PLUGIN_BACKOFFICE_CUSTOMER)
        kv(customer ${PLUGIN_BACKOFFICE_CUSTOMER})
    endif()
    if(PLUGIN_BACKOFFICE_PLATFORM)
